{
  "name": "led-view",
  "version": "0.0.1",
  "description": "led-view",
  "scripts": {
    "bs-front": "rm .bootstraprc && cp .bootstraprc-front .bootstraprc",
    "bs-admin": "rm .bootstraprc && cp .bootstraprc-admin .bootstraprc",
    "start:front": "npm run bs-front && node server-front.js",
    "start:admin": "npm run bs-admin && node server-admin.js",
    "build": "npm run build:front && npm run build:admin",
    "build:front": "npm run bs-front && NODE_ENV=production webpack --bail --config webpack-front/prod.config.js",
    "build:admin": "npm run bs-admin && NODE_ENV=production webpack --bail --config webpack-admin/prod.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src-admin",
    "test": "NODE_ENV=test mocha 'test/UnitTest/**/*.js' --recursive --compilers js:babel-core/register --require ./test/UnitTest/setup.js --require ./test/UnitTest/css-null-compiler.js",
    "test:db": "./node_modules/.bin/babel-node test.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HoritaWorks/ledview_front"
  },
  "homepage": "https://github.com/HoritaWorks/ledview_front",
